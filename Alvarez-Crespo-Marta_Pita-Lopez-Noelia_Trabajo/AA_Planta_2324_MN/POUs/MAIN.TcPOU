    <Declaration><![CDATA[PROGRAM MAIN
VAR
	bON: BOOL; 
	bOFF: BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(*
  	  ESCOLA POLITÉCNICA DE ENXEÑARÍA DE FERROL			
