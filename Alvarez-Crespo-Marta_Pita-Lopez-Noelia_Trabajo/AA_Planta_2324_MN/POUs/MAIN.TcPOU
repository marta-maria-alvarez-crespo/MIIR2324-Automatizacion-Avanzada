POU_StopBladePyP();
POU_Contadores();

IF GVL_General.bMarcha THEN
	bON := TRUE;
END_IF

IF NOT GVL_General.bParo THEN
	bON := FALSE;
	POU_FinDeCiclo();
END_IF

IF bON AND GVL_General.bSetaEmergencia THEN
	
	POU_OnFabrica();
	POU_ProcesoEntrada();
	POU_PickAndPlace();
	POU_PositionerAzul();
	POU_Parada();
	PRG_Turntable();
	PRG_Almacen();
	PRG_Cintas();
	PRG_PaneldeControl();
	
	IF GVL_Almacen.bSetaEmergencia THEN
		PRG_RetiradaAlmacen();
	END_IF
	
END_IF

IF NOT GVL_General.bSetaEmergencia THEN
	POU_SetaEmergencia();
END_IF





























































